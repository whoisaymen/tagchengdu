  "homepage": "https://github.com/sanity-io/sanity-template-nextjs-clean#readme",
  "workspaces": [
    "studio",
    "frontend"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.4"
  },
  "dependencies": {
    "@sanity/document-internationalization": "^3.3.3",
    "geist": "^1.4.2",
